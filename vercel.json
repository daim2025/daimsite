{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/idol-demo-luxury.html"
    },
    {
      "src": "/index",
      "dest": "/idol-demo-luxury.html"
    },
    {
      "src": "/luxury",
      "dest": "/index-luxury-evolved.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "outputDirectory": ".",
  "public": true
}
